{
    "es5": true,
    "expr": true,
    "maxerr": 50,
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": false,
    "immed": false,
    "indent": 2,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": false,
    "plusplus": false,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": false,
    "maxparams": 5,
    "maxdepth": 3,
    "maxlen": 160,
    "browser": true,
    "devel": false,
    "jquery": false,
    "node": true,

    "predef": [
        "delete",
        "define",
        "xdescribe",
        "describe",
        "it",
        "xit",
        "afterAll",
        "beforeAll",
        "beforeEach",
        "afterEach",
        "default",
        "spyOn",
        "after",
        "before"
    ]
}
